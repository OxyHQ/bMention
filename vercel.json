{
    "version": 2,
    "builds": [
        {
            "src": "cmd/bskyweb/main.go",
            "use": "@vercel/go",
            "config": {
                "functions": {
                    "main.go": {
                        "handler": "Handler"
                    }
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.go"
        }
    ]
}