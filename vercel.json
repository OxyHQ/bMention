{
    "version": 2,
    "builds": [
        {
            "src": "cmd/bskyweb/main.go",
            "use": "@vercel/go"
        },
        {
            "src": "public/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}